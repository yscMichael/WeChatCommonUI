Page{
  width: 100%;
  height: 100%;
  background-color:rgb(233, 75, 36);
}

.main-view{
  background-color:#eee;
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

/* .bottom-view{
  background-color: blue;
